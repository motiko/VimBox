" call <sid>X('MatchParen',   s:syntax_bg,     s:hue_6_2,          '')
" let g:syntastic_javascript_checkers = ['javascript/eslint']
set foldmethod=indent   
set foldnestmax=10
set nofoldenable
set foldlevel=2
